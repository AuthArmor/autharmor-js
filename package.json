{
    "name": "autharmor-sdk",
    "version": "4.0.0",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "solid": {
            "import": "./dist/index.jsx"
        },
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "require": "./dist/index.cjs"
    },
    "license": "MIT",
    "homepage": "https://github.com/AuthArmor/autharmor-jsclient-sdk#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/AuthArmor/autharmor-jsclient-sdk.git"
    },
    "files": [
        "./dist"
    ],
    "scripts": {
        "build": "tsup",
        "prepublishOnly": "pnpm build",
        "test": "vitest",
        "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\"",
        "update-deps": "pnpm up -Li",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@solid-primitives/i18n": "^1.2.4",
        "qrcode": "^1.5.3",
        "solid-js": "^1.7.2"
    },
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "devDependencies": {
        "@types/qrcode": "^1.5.0",
        "esbuild": "^0.17.15",
        "postcss": "^8.4.23",
        "prettier": "^2.8.7",
        "tsup": "^6.7.0",
        "tsup-preset-solid": "^0.1.8",
        "typescript": "^5.0.3",
        "typescript-plugin-css-modules": "^5.0.0"
    }
}
